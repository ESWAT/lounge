<div id="chan-{{id}}" data-title="{{name}}" data-id="{{id}}" data-type="{{type}}" data-target="#chan-{{id}}" class="chan">
	<div class="header">
		<button class="lt" aria-label="Toggle channel list"></button>
		<span class="title">Search results for</span>
		<span class="topic">{{options.query}}</span>
	</div>
	<div class="chat">
		<div class="messages"></div>
	</div>
</div>
